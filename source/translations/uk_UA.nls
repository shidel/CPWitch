      <NegCurrFormat Value="1"/>
      <DecimalSeparator Value=","/>
      <CurrencyDecimals Value="2"/>
      <DateSeparator Value="."/>
      <TimeSeparator Value=":"/>
      <ListSeparator Value=";"/>
      <CurrencyString Value="₴"/>
      <ShortDateFormat Value="dd.mm.yyyy"/>
      <LongDateFormat Value="d mmmm yyyy &quot;р.&quot;"/>
      <TimeAMString Value=""/>
      <TimePMString Value=""/>
      <ShortTimeFormat Value="HH:mm"/>
      <LongTimeFormat Value="HH:mm:ss"/>
      <ShortMonthNames>
        <Index_1 Value="січ"/>
        <Index_2 Value="лют"/>
        <Index_3 Value="бер"/>
        <Index_4 Value="квіт"/>
        <Index_5 Value="трав"/>
        <Index_6 Value="черв"/>
        <Index_7 Value="лип"/>
        <Index_8 Value="серп"/>
        <Index_9 Value="вер"/>
        <Index_10 Value="жовт"/>
        <Index_11 Value="лист"/>
        <Index_12 Value="груд"/>
      </ShortMonthNames>
      <LongMonthNames>
        <Index_1 Value="січень"/>
        <Index_2 Value="лютий"/>
        <Index_3 Value="березень"/>
        <Index_4 Value="квітень"/>
        <Index_5 Value="травень"/>
        <Index_6 Value="червень"/>
        <Index_7 Value="липень"/>
        <Index_8 Value="серпень"/>
        <Index_9 Value="вересень"/>
        <Index_10 Value="жовтень"/>
        <Index_11 Value="листопад"/>
        <Index_12 Value="грудень"/>
