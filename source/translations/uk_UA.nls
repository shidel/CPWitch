      <ShortDayNames>
        <Index_1 Value="Нд"/>
        <Index_2 Value="Пн"/>
        <Index_3 Value="Вт"/>
        <Index_4 Value="Ср"/>
        <Index_5 Value="Чт"/>
        <Index_6 Value="Пт"/>
        <Index_7 Value="Сб"/>
      </ShortDayNames>
      <LongDayNames>
        <Index_1 Value="неділя"/>
        <Index_2 Value="понеділок"/>
        <Index_3 Value="вівторок"/>
